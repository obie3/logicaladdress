{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "format": "prettier --write 'src/**/*.js'",
    "check": "prettier --check 'src/**/*.js'"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ]
  },
  "dependencies": {
    "expo": "^36.0.0",
    "expo-constants": "~8.0.0",
    "expo-font": "~8.0.0",
    "expo-image-picker": "~8.0.0",
    "expo-permissions": "~8.0.0",
    "pretty-quick": "^2.0.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.1.tar.gz",
    "react-native-app-intro-slider": "^2.0.1",
    "react-native-check-box": "^2.1.7",
    "react-native-confirmation-code-field": "^5.1.0",
    "react-native-dropdownalert": "^4.2.1",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-maps": "0.26.1",
    "react-native-reanimated": "~1.4.0",
    "react-native-simple-dialogs": "^1.1.0",
    "react-native-svg": "9.13.3",
    "react-native-user-avatar": "^1.0.4",
    "react-native-web": "^0.11.7",
    "react-navigation": "^3.11.0",
    "react-redux": "^7.1.0",
    "reanimated-bottom-sheet": "^1.0.0-alpha.19",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "rn-placeholder": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "^8.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "^4.0.7",
    "lint-staged": "^9.5.0",
    "prettier": "1.19.1"
  },
  "private": true
}
